{
  "name": "aisteth-backend",
  "version": "1.0.0",
  "description": "Backend API for AISteth medical AI assistant platform",
  "main": "dist/server.js",
  "scripts": {
    "dev": "set PRISMA_CLIENT_ENGINE_TYPE=wasm && nodemon src/server.ts",
    "build": "tsc",
    "start": "node dist/server.js",
    "test": "jest",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "migrate": "prisma migrate dev",
    "generate": "prisma generate",
    "db:seed": "ts-node prisma/seed.ts"
  },
  "keywords": [
    "medical-ai",
    "healthcare",
    "ai-assistant",
    "medical-coding",
    "ohip-billing",
    "physician-tools",
    "api"
