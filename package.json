{
  "name": "aisteth",
  "version": "1.0.0",
  "description": "AI-powered medical assistant platform for healthcare providers",
  "private": true,
  "workspaces": [
    "frontend",
    "backend"
  ],
  "scripts": {
    "dev": "concurrently \"npm run dev:backend\" \"npm run dev:frontend\"",
    "dev:frontend": "cd frontend && npm run dev",
    "dev:backend": "cd backend && npm run dev",
    "build": "cd frontend && npm install && npm run build ",
    "build:frontend": "cd frontend && npm run build",
    "build:backend": "cd backend && npm run build",
    "install:all": "npm install && cd frontend && npm install && cd ../backend && npm install",
    "setup": "npm run install:all && npm run setup:prisma && npm run setup:frontend",
    "setup:prisma": "cd backend && $env:PRISMA_CLIENT_ENGINE_TYPE='wasm'; npx prisma generate",
    "setup:frontend": "cd frontend && npm install @tailwindcss/typography @tailwindcss/forms @tanstack/react-query",
    "start": "npm run setup && npm run dev",
    "clean": "npm run clean:node_modules && npm run clean:prisma",
    "clean:node_modules": "rimraf node_modules frontend/node_modules backend/node_modules",
    "clean:prisma": "cd backend && rimraf node_modules/.prisma",
    "reset": "npm run clean && npm run setup",
    "db:generate": "cd backend && $env:PRISMA_CLIENT_ENGINE_TYPE='wasm'; npx prisma generate",
    "db:push": "cd backend && $env:PRISMA_CLIENT_ENGINE_TYPE='wasm'; npx prisma db push",
    "db:studio": "cd backend && $env:PRISMA_CLIENT_ENGINE_TYPE='wasm'; npx prisma studio"
  },
  "devDependencies": {
    "@prisma/client": "^6.12.0",
    "concurrently": "^8.2.2",
    "prisma": "^6.12.0",
    "rimraf": "^5.0.5"
  },
  "keywords": [
    "medical-ai",
    "healthcare",
    "ai-assistant",
    "medical-coding",
    "ohip-billing",
    "physician-tools"
  ],
  "author": "AISteth Team",
  "license": "MIT",
  "dependencies": {
    "@mermaid-js/mermaid-cli": "^11.6.0",
    "@types/node": "^24.1.0",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6"
  }
}
